@model IEnumerable<ViewAccountDetailsVM>
@{
    ViewData["Title"] = "SSN";
}

<h1>SSN</h1>


@if (Model != null)
{
    <table class="table table-striped">
        <tr>
            <th>Account ID</th>
            <th>Customer ID</th>
            <th>Account Type</th>
            <th>Balance</th>
            <th>Actions</th>
        </tr>

        @foreach (ViewAccountDetailsVM a in Model)
        {
            <tr>
                <td> @a.AccountID</td>
                <td> @a.CustomerID</td>
                <td> @a.AccountType</td>
                <td> @a.Balance</td>

                <td>
                    @Html.ActionLink("Delete", "DeleteAccount", new { id = @a.AccountID })
                </td>
            </tr>

        }
    </table>
}