@model IEnumerable<Transaction>
@{
    ViewData["Title"] = "viewTransaction";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>viewTransaction</h1>
<table class=" table table-striped">
    <tr>
        <th>ID</th>
        <th>Account ID</th>
        <th>Account ID 2</th>
        <th>Transaction type</th>
        <th>Amount</th>
    </tr>
    @foreach (Transaction t in Model)
        {
<tr>
    <td>@t.ID</td>
    <td>@t.Account_ID</td>
    <td>@t.Account_ID_2</td>
    <td>@t.Transaction_Type</td>
    <td>@t.Amount</td>
</tr>
    }
</table>

